{
  "name": "rmurray-iis",
  "version": "0.1.2",
  "author": "rmurray",
  "summary": "IIS Provider and Type designed for compatability with as ride a range of Windows Server OS's as possible.",
  "license": "MIT",
  "source": "https://github.com/rossmurr4y/puppet-iis",
  "project_page": "(https://github.com/rossmurr4y/puppet-iis)",
  "issues_url": "https://github.com/rossmurr4y/puppet-iis/issues",
  "operatingsystem_support": [
    {
